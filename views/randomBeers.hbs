<div>
    {{#each randomBeers}}
        <img src="{{image_url}}" alt="{{name}}"/>
        <h3>{{name}}</h3>
        <p>{{description}}</p>
        <p>{{tagline}}</p>
        <h5>Food pairing</h5>
        <ul>
            {{#each food_pairing}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <p>{{brewer_tips}}</p>
    {{/each}}
</div>